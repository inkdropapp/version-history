{
  "name": "inkdrop-version-history",
  "version": "3.23.2",
  "description": "The version history of Inkdrop for desktop",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:json && npm run build:readme && npm run build:meta",
    "build:readme": "node index.js",
    "build:json": "js-yaml history.yml > history.json",
    "build:meta": "node genmeta.js",
    "release": "env $(cat .env | xargs) node release.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "inkdrop"
  ],
  "author": "Takuya Matsuyama <t@inkdrop.info>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "prettier": "1.10.2",
    "js-yaml": "^3.10.0"
  },
  "dependencies": {
    "dateformat": "^3.0.3"
  }
}
