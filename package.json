{
  "name": "inkdrop-version-history",
  "version": "4.0.1",
  "description": "The version history of Inkdrop for desktop",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkdropapp/version-history.git"
  },
  "scripts": {
    "build": "npm run build:json && npm run build:readme && npm run build:meta && npm run build:changelog",
    "build:readme": "node index.js",
    "build:json": "js-yaml history.yml > history.json",
    "build:meta": "node genmeta.js",
    "build:changelog": "node changelog.js",
    "release": "env $(cat .env | xargs) node release.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "docs:deploy": "curl 'https://api.netlify.com/api/v1/sites/b75510a9-302c-4ceb-848c-c9f517ae0152/builds' -H 'Origin: https://app.netlify.com' -H 'Authorization: Bearer 2664424fc5c48655dfe7fd658bfa156dcf9acdaa579a9db96cb23aafdf233954' -H 'Content-Type: application/json' --data-binary '{\"clear_cache\":false}' --compressed | jq ."
  },
  "keywords": [
    "inkdrop"
  ],
  "author": "Takuya Matsuyama <t@inkdrop.app>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "js-yaml": "^3.10.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "dateformat": "^3.0.3"
  }
}
